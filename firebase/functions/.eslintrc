{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true,
    "es6": true
    },
  "rules": {
    "comma-dangle": [2, "only-multiline"],
    "eqeqeq": [2, "allow-null"],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "keyword-spacing": 2,
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-spaced-func": 2,
    "object-curly-spacing": [2, "always"],
    "semi": [2, "always"],
    "space-before-blocks": 2,
    "space-infix-ops": [2, {"int32Hint": false}],
    "no-console": 0
  }
}
